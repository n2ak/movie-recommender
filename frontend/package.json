{
  "name": "frontend",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "db:migrate": "cd packages/database && npx prisma migrate dev --name init",
    "db:generate": "cd packages/database && npx prisma generate",
    "dockerize": "docker build -f apps/web/Dockerfile -t webapp .",
    "db:seed": "cd packages/database && npx prisma generate && npx prisma migrate reset --force && npx prisma migrate dev --name init && npx ts-node src/scripts/seed.ts",
    "db:dump": "cd packages/database && npx ts-node src/scripts/dump.ts",
    "clean-install": "rimraf ./node_modules && pnpm i",
    "clean": "find . -name \"node_modules\" -type d -prune -exec rm -rf {} +"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "turbo": "^2.5.4",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@10.16.0",
  "engines": {
    "node": ">=18"
  }
}